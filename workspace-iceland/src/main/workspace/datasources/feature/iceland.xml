    <Primitive path="lat" mapping="lat" />
    <Primitive path="long" mapping="long" />
    <Primitive path="dms_lat" mapping="dms_lat" />
    <Primitive path="dms_long" mapping="dms_long" />
    <Primitive path="mgrs" mapping="mgrs" />
