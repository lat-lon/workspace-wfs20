    <Primitive path="full_name_nd_rg" mapping="full_name_nd_rg" />
    <Primitive path="note" mapping="note" />
    <Primitive path="modify_date" mapping="modify_date" />
    <Primitive path="display" mapping="display" />
    <Primitive path="name_rank" mapping="name_rank" />
    <Primitive path="name_link" mapping="name_link" />
    <Primitive path="transl_cd" mapping="transl_cd" />
    <Primitive path="nm_modify_date" mapping="nm_modify_date" />
    <Primitive path="f_efctv_dt" mapping="f_efctv_dt" />
    <Primitive path="f_term_dt" mapping="f_term_dt" />
    <Geometry path="geometry" mapping="geometry">
      <StorageCRS srid="4326" dim="2D">EPSG:4326</StorageCRS>
    </Geometry>
  </FeatureTypeMapping>

  <FeatureTypeMapping table="osm_administrative">
    <FIDMapping prefix="osm_administrative_">
      <Column name="osm_id" type="integer" />
      <UUIDGenerator />
    </FIDMapping>
    <Primitive path="osm_id" mapping="osm_id" />
    <Primitive path="admin_level" mapping="admin_level" />
    <Primitive path="name" mapping="name" />
    <Primitive path="place" mapping="place" />
    <Primitive path="way_area" mapping="way_area" />
    <Geometry path="geometry" mapping="geometry">
      <StorageCRS srid="4326" dim="2D">EPSG:4326</StorageCRS>
    </Geometry>
  </FeatureTypeMapping>

  <FeatureTypeMapping table="osm_protected_area">
    <FIDMapping prefix="osm_protected_area_">
      <Column name="osm_id" type="integer" />
      <UUIDGenerator />
    </FIDMapping>
    <Primitive path="osm_id" mapping="osm_id" />
    <Primitive path="boundary" mapping="boundary" />
    <Primitive path="leisure" mapping="leisure" />
