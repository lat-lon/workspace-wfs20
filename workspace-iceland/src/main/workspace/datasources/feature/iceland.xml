    <Primitive path="fc" mapping="fc" />
    <Primitive path="dsg" mapping="dsg" />
    <Primitive path="pc" mapping="pc" />
    <Primitive path="cc1" mapping="cc1" />
    <Primitive path="adm1" mapping="adm1" />
    <Primitive path="pop" mapping="pop" />
    <Primitive path="elev" mapping="elev" />
    <Primitive path="cc2" mapping="cc2" />
    <Primitive path="nt" mapping="nt" />
    <Primitive path="lc" mapping="lc" />
    <Primitive path="short_form" mapping="short_form" />
    <Primitive path="generic" mapping="generic" />
    <Primitive path="sort_name_ro" mapping="sort_name_ro" />
